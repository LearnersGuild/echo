{
  "name": "game",
  "version": "0.1.0",
  "description": "Game Service",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearnersGuild/game.git"
  },
  "author": {
    "name": "Jeffrey Wescott",
    "email": "jeffrey@learnersguild.org"
  },
  "scripts": {
    "build": "mkdir -p dist && webpack --config config/webpack-production-config.js",
    "icons:fetch": "mkdir -p dist && curl -s https://brand.learnersguild.org/icontags > dist/icons-metadata.json",
    "db:create": "./node_modules/.bin/babel-node ./db/create.js",
    "db:drop": "./node_modules/.bin/babel-node ./db/drop.js",
    "db:migrate:configure": "node ./db/config.js > ./db/database.json",
    "db:migrate": "npm run db:migrate:configure && node ./node_modules/.bin/rethink-migrate -r db",
    "dev:testdata": "./node_modules/.bin/babel-node test/generateTestData",
    "dev:testdata:goals": "./node_modules/.bin/babel-node test/generateTestGoals",
    "start": "npm run icons:fetch && node server",
    "workers": "./node_modules/.bin/babel-node ./server/workers",
    "postinstall": "npm run build",
    "test": "./node_modules/.bin/xo && NODE_ENV=test ./node_modules/.bin/ava --serial",
    "test:watch": "./node_modules/.bin/xo && NODE_ENV=test ./node_modules/.bin/ava --serial --watch"
  },
  "keywords": [
    "game"
  ],
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/LearnersGuild/game/issues"
  },
  "homepage": "https://github.com/LearnersGuild/game#readme",
  "dependencies": {
    "@learnersguild/idm-jwt-auth": "^0.2.4",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "bourbon": "^4.2.6",
    "bourbon-neat": "^1.7.4",
    "bull": "^1.0.0-rc1",
    "chokidar": "^1.4.3",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "css-loader": "^0.23.0",
    "css-modules-require-hook": "^3.0.0",
    "dotenv": "^2.0.0",
    "eslint-config-xo-react": "^0.5.0",
    "eslint-plugin-react": "^4.1.0",
    "express": "^4.13.3",
    "express-graphql": "^0.4.9",
    "express-sslify": "^1.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "graphql": "^0.4.18",
    "graphql-custom-types": "^0.3.0",
    "history": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^8.0.4",
    "json-loader": "^0.5.4",
    "juration": "0.0.1",
    "later": "^1.2.0",
    "moment": "^2.12.0",
    "moment-timezone": "^0.5.3",
    "node-sass": "3.4.2",
    "normalize.css": "^4.0.0",
    "normalizr": "^2.0.1",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss": "^5.0.17",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.0",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "raven": "^0.10.0",
    "raven-js": "^2.1.1",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.5",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "react-toolbox": "github:learnersguild/react-toolbox#lg-master",
    "redux": "^3.0.5",
    "redux-auth-wrapper": "^0.3.0",
    "redux-form": "^4.2.0",
    "redux-thunk": "^1.0.3",
    "resolve-url-loader": "^1.4.3",
    "rethink-migrate": "^1.3.1",
    "rethinkdb": "2.2.3",
    "rethinkdbdash": "^2.2.18",
    "sass-loader": "^3.2.0",
    "sass-resources-loader": "^1.0.2",
    "serve-static": "^1.10.0",
    "socketcluster-client": "^4.3.16",
    "socketcluster-server": "^4.1.10",
    "style-loader": "^0.13.0",
    "toolbox-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "yup": "^0.17.1"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "bluebird": "^3.3.5",
    "eslint": "^2.2.0",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.3.7",
    "eslint-plugin-react": "^3.16.1",
    "factory-girl": "^3.0.1",
    "faker": "^3.1.0",
    "jsdom": "^8.0.2",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.1",
    "xo": "^0.12.1"
  },
  "engines": {
    "node": "5.6.x",
    "npm": "3.7.x"
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "comma-dangle": [
        0
      ],
      "no-negated-condition": [
        0
      ],
      "react/jsx-indent": [
        2,
        2
      ],
      "react/jsx-indent-props": [
        2,
        2
      ],
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ]
    },
    "esnext": true,
    "space": true,
    "semicolon": false
  },
  "ava": {
    "files": [
      "**/__tests__/*-test.js"
    ],
    "failFast": false,
    "verbose": true,
    "require": [
      "babel-core/register",
      "babel-polyfill",
      "./config/testConfig"
    ]
  }
}
